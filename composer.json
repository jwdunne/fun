{
  "name": "jwdunne/fun",
  "description": "Functional utility belt for PHP",
  "type": "library",
  "require": {
    "php": "^7.4"
  },
  "require-dev": {
    "infection/infection": "^0.16.3",
    "steos/quickcheck": "dev-master",
    "psy/psysh": "^0.10.4"
  },
  "autoload": {
    "files": [
      "src/Fun.php",
      "src/Properties.php",
      "src/Relations.php"
    ],
    "psr-4": {
      "Fun\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "James Dunne",
      "email": "james@thefreshuk.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:jwdunne/php-quickcheck.git"
    }
  ]
}
